@model IEnumerable<PBLVII.Models.Programador>

@{
    ViewBag.Title = "Gerar PDF";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.ActionLink("Buscar Atividades", "BuscarAtividades", new { id = item.ProgramadorId })
            </td>
        </tr>
    }

</table>
