
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>LayoutRelatorios</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body  background="~/img/imagemm.jpg">
    <div class="container">
        <div class="col-md-4 ">
            <img class="img-responsive" src="~/img/senai_bahia.png" alt="Logo Senai">
        </div>
       
        <div class="container" id="fundo">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; Relatório - @DateTime.Now.Year - Let's Que Let's</p>
            </footer>
        </div>

    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
